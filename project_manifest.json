    },
    "ui_ux_settings": {
        "variants_count": 5,
        "default_theme": "Modern-Dark-Agentic",
        "enable_3d": true
    },
    "debugger_settings": {
    "enabled": true,
    "log_level": "INFO",
    "enforced_tags": ["div", "section", "main"],
    "clean_empty_classes": true,
    "repair_truncated_tags": true
},
"editor_settings": {
    "template_dir": "templates/components",
    "placeholders": {
        "{{BRAND_NAME}}": "brand",
        "{{MISSION_STMT}}": "mission",
        "{{OWNER_NAME}}": "owner_name"
