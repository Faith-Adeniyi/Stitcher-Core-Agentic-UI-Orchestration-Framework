        "name": "Stitcher-Core Professional Portfolio",
        "owner": "Faith Adeniyi",
        "target_deadline": "2026-03-31"
    },
    "orchestrator_settings": {
        "model": "qwen2.5-coder:7b", 
        "writing_model": "llama3.2:1b",
        "role": "Lead Technical Recruiter & Web Architect",
        "available_components": [
            "hero_section", 
            "tech_stack_grid", 
            "agent_terminal_demo"
        ]
    },
    "researcher_settings": {
        "source_data": "data/brand_memory.json",
